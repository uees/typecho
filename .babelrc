{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-object-assign",
    "external-helpers",
    [
      "transform-es2015-classes",
      {
        "loose": false
      }
    ],
    [
      "transform-es2015-modules-commonjs",
      {
        "loose": false
      }
    ]
  ]
}